package com;

class Container<T extends Number>{
	T value;
	
	public T getValue() {
		return value;
	}

	public void setValue(T value) {
		this.value = value;
	}

	public void show() {
		System.out.println(value.getClass().getName());
	}
	
}

public class CollectionDemo{
	

	public static void main(String[] args) {
		Container<Double> obj = new Container<>();
		obj.value=9.9;
		obj.show();
	}

}
